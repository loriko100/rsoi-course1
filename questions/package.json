{
  "name": "lab",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./server/app.js",
    "test": "NODE_ENV=test jest --forceExit ",
    "test-coverage": "cross-env NODE_ENV=test jest --forceExit --coverage",
    "pretest": "cross-env NODE_ENV=test npm run migrate",
    "migrate": "npx sequelize-cli cross-env NODE_ENV=developmentquestions db:migrate",
    "db:create": "npx sequelize-cli cross-env NODE_ENV=developmentquestions db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:seed:all": "cross-env NODE_ENV=developmentquestions npx sequelize-cli db:seed:all",
    "db:makeQuestion": "npx sequelize-cli  cross-env NODE_ENV=developmentquestions model:generate --name Question --attributes title:string,text:string,uid:integer,date:string,tag:string --force",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "heroku": "^7.35.0",
    "jest": "^24.9.0",
    "path": "^0.12.7",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "winston": "^3.1.0",
    "yargs": "^15.1.0"
  },
  "engines": {
    "node": "8.16.x",
    "npm": "6.4.x"
  }
}
